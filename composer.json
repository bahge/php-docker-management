{
    "name": "bahge/dkman",
    "description": "Pacote para geração de arquivos docker com estrutura básica de configuração.",
    "autoload": {
        "psr-4": {
            "Bahge\\Dkman\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Leandro Garcia",
            "email": "leandrobage@gmail.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "pestphp/pest": "^2.17"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "php-di/php-di": "^7.0"
    }
}
